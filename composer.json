{
	"name" : "sunhill/files",
	"description" : "The sunhill file manager",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Klaus Dimde",
			"email" : "klaus@dimde.info"
		}
	],
	"minimum-stability" : "dev",
       "require": {
                "sunhill/basic":"*",
                "sunhill/orm":"*"
        },
        "require-dev" : {
                "phpunit/phpunit" : "^10",
                "orchestra/testbench" : "^8.5",
                "doctrine/dbal": "3.7.x"                
        },
	"autoload" : {
		"psr-4" : {
			"Sunhill\\Files\\Tests\\" : [
				"tests",
				"tests/lib",
				"tests/objects"
			],
			"Sunhill\\Files\\Test\\" : [
				"tests",
				"tests/lib",
				"tests/objects"
			],
			"Sunhill\\Files\\" : "src"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Sunhill\\Files\\FilesServiceProvider"
			]
		}
	}
}